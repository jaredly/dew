{
  "name": "dew-node",
  "version": "0.0.2",
  "description": "brew for docker",
  "main": "index.js",
  "bin": {
    "dew": "bin/dew"
  },
  "config": {
    "blanket": {
      "pattern": "/",
      "data-cover-never": "node_modules"
    }
  },
  "scripts": {
    "test": "mocha --recursive test",
    "tdd": "mocha -w -R min --recursive test 2>&1 | simple-stacktrace",
    "tdd2": "watchy -w . -i '/\\.|node_modules|\\.json$' -- mocha -R min --recursive test 2>&1 | simple-stacktrace",
    "autocover": "watchy -w . -i '/\\.|node_modules|\\.json|coverage$' -- npm run cover",
    "cover": "istanbul cover _mocha -- --recursive test",
    "cover2": "mocha --require blanket -R html-cov --recursive test > coverage.html",
    "dev-gitlab": "bash scripts/dev-gitlab.sh",
    "dev-postgresql": "watchy -w . -i '/\\.|node_modules|\\.json$' -- sudo dew reinstall postgresql --namespace pgtest 2>&1 | simple-stacktrace"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/keyvanfatehi/dew.git"
  },
  "author": "Keyvan Fatehi <keyvanfatehi@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^2.2.2",
    "dockerode": "^2.0.1",
    "lodash": "^2.4.1",
    "minimist": "^0.2.0",
    "node-localstorage": "^0.3.4",
    "progress": "^1.1.7",
    "rimraf": "^2.2.8",
    "uri-js": "^1.4.2"
  },
  "devDependencies": {
    "chai": "^1.9.1",
    "istanbul": "^0.3.0",
    "mocha": "^1.21.3",
    "nock": "^0.43.1",
    "simple-stacktrace": "^0.2.1",
    "sinon": "^1.10.3",
    "watchy": "^0.5.2"
  }
}
